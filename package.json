{
  "name": "thepress",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "styles": "node-sass assets/scss -o assets/css --output-style",
    "scripts": "mkdir -p assets/js && concat -o assets/js/all.js assets/js/file1.js assets/js/file2.js",
    "watch:styles": "onchange \"assets/scss\" -- npm run styles",
    "watch:scripts": "onchange \"assets/js\" -- npm run scripts",
    "serve": "node assets/build/browser-sync.js config.json browserSync",
    "dev": "npm-run-all -p serve watch:styles"
  },
  "keywords": [
    "bootstrap 4",
    "WordPress"
  ],
  "author": "Anderson Nascimento",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "concat": "^1.0.3",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0"
  }
}
